<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <connectionStrings>
    <add name="Redis.ConnectionString" connectionString="127.0.0.1:6379" providerName="David.Common.Redis" />
  </connectionStrings>
  <appSettings>
    <add key="Redis.KeyPrefix" value="DavidNews:"/>
    <add key="Redis.ExpirationIntervalInMinutes" value="5"/>
  </appSettings>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.1" />
    </startup>
</configuration>